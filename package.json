{
	"name": "qtmusic",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "tauri dev",
		"build": "tauri build",
		"vbuild": "vite build",
		"vdev": "vite",
		"preview": "vite preview",
		"tauri": "tauri",
		"commit": "git-cz",
		"lint:lint-staged": "lint-staged",
		"lint:eslint": "eslint --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write --log-level warn \"src/**/*.{js,ts,json,jsx,tsx,css,scss,vue,html,md}\"",
		"lint:prettierall": "prettier --write --log-level warn \".\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
			"prettier --write--parser json"
		],
		"package.json": [
			"prettier --write"
		],
		"*.{scss,less,styl}": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.md": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@tauri-apps/api": "^2.2.0",
		"@tauri-apps/plugin-global-shortcut": "^2.2.0",
		"@tauri-apps/plugin-opener": "^2.2.4",
		"@tauri-apps/plugin-window-state": "^2.2.0",
		"axios": "^1.7.9",
		"cookie-js": "^0.0.1",
		"howler": "^2.2.4",
		"js-cookie": "^3.0.5",
		"qtmusic": "file:",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.28.2",
		"sass": "^1.83.4",
		"zustand": "^4.5.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@iconify-json/material-symbols": "^1.2.12",
		"@iconify-json/solar": "^1.2.2",
		"@iconify/react": "^5.2.0",
		"@tauri-apps/cli": "^2.2.5",
		"@types/howler": "^2.2.12",
		"@types/js-cookie": "^3.0.6",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.17.14",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@unocss/preset-icons": "^0.65.4",
		"@unocss/preset-rem-to-px": "^0.61.9",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.18",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"husky": "^9.1.7",
		"lint-staged": "^15.4.1",
		"lodash-es": "^4.17.21",
		"postcss": "^8.5.1",
		"postcss-mobile-forever": "^4.3.2",
		"prettier": "^3.4.2",
		"stylelint": "^16.13.2",
		"stylelint-config-recess-order": "^5.1.1",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-prettier": "^5.0.2",
		"typescript": "^5.7.3",
		"unocss": "^0.61.9",
		"vite": "^5.4.11"
	}
}
